{
    "name": "konekt/kampaign",
    "description": "Simple popup ad manager",
    "license": "MIT",
    "keywords": ["kampaign", "artkonekt", "mailchimp"],
    "authors": [
        {
            "name": "Lajos Fazakas",
            "email": "lajos@artkonekt.com"
        },
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "5.5 - 7.0",
        "mailchimp/mailchimp": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "Konekt\\Kampaign\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\Kampaign\\Tests\\": "tests/unit"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
